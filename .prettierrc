{
	"useTabs": true,
	"singleQuote": false,
	"jsxSingleQuote": false,
	"trailingComma": "none",
	"printWidth": 100,
	"semi": false,
	"bracketSpacing": true,
	"htmlWhitespaceSensitivity": "strict",
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
